let numero = parseInt(prompt("Ingresa un número entre 1 y 12:"));

let meses = [
  "Enero", "Febrero", "Marzo", "Abril", "Mayo", "Junio",
  "Julio", "Agosto", "Septiembre", "Octubre", "Noviembre", "Diciembre"
];

if (isNaN(numero)) {
  alert("Eso no es un número válido.");
} else if (numero >= 1 && numero <= 12) {
  alert("El mes correspondiente es: " + meses[numero - 1]);
} else {
  alert("Error: el número debe estar entre 1 y 12.");
}
